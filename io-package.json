    },
    "objects": [

    ],
  "instanceObjects": [
    {
      "_id":  "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id":  "info.connection",
      "type": "state",
      "common": {
        "role":  "indicator.connected",
        "name":  "If connected to FHEM server",
        "type":  "boolean",
        "read":  true,
        "write": false,
        "def":   false
      },
      "native": {}
    },
    {
      "_id":  "info.resync",
      "type": "state",
      "common": {
        "role":  "state",
        "name":  "read all states from FHEM anew",
        "type":  "boolean",
        "read":  true,
        "write": true,
        "def":   false
      },
      "native": {}
    }
  ]
}
